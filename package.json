{
  "type": "module",
  "private": true,
  "name": "quzelly",
  "version": "1.0.0",
  "description": "1Piece팀의 Quzelly 프로젝트",
  "scripts": {
    "start": "pnpm dev --open",
    "dev": "vite --host",
    "build": "tsc && vite build",
